#include<iostream>
using namespace std;
int main(){
	long long t;
	cin>>t;
	while(t--){
	long long n,k,d;
	cin>>n>>k>>d;
	long long a[n],i;
	for(i=0;i<n;i++){
		cin>>a[i];
	}
	long long s=0,p;
	for(i=0;i<n;i++){
		s=s+a[i];
}
	if(s<k){
		p=0;
	}
	else{
		p=s/k;
	}
	if(p>d)
	{
		cout<<d<<endl;
	}
	else{

	cout<<p<<endl;
}

}	return 0;

}
